alignment top_right
background yes
border_width 0
border_inner_margin 0
color1 ffffff
color2 AAAAAA
color3 f94d4d # rojo
color4 f99d4d # naranja
color5 85c6c4 # cyan lavado
color6 b24df9 # alert
cpu_avg_samples 4
default_color white
default_graph_size 0 25
default_outline_color blue
default_shade_color white
disable_auto_reload no
double_buffer yes
draw_borders no
draw_graph_borders no
draw_outline no
draw_shades no
extra_newline yes
font DejaVu Sans Mono:size=8
gap_x 8
gap_y 0
maximum_width 200
minimum_size 200 730
net_avg_samples 1
no_buffers yes
out_to_console no
out_to_stderr no
net_avg_samples 2
nvidia yes
own_window yes
own_window_colour 000000
own_window_argb_visual yes
own_window_type normal
own_window_transparent yes
own_window_hints undecorated, below, above, sticky, skip_taskbar, skip_pager
short_units yes
stippled_borders 0
top_cpu_separate no
total_run_times 0
update_interval 1
uppercase no
use_spacer none
show_graph_scale no
show_graph_range no
use_xft yes
xftfont Hack:size=8
xftalpha 0.35
lua_load ~/.conky/conky_orange/conky_orange.lua
# lua_load ~/.conky/conky_orange/cava.lua
lua_draw_hook_pre main
lua_draw_hook_post main_bars
